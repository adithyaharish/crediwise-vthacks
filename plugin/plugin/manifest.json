{
  "manifest_version": 3,
  "name": "Checkout Trigger",
  "version": "1.0.0",
  "description": "Triggers on checkout pages and sends a webhook",
  "permissions": ["tabs", "scripting"],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Checkout Trigger"
  },
  "icons": {
    "16": "icon16.png"
  }
}


